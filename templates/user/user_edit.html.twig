{% extends 'base.html.twig' %}

{% block title %}{{ 'user.edit'|trans }}{% endblock %}

{% block body_id 'user_edit' %}

{% block main %}

    {{ form_start(form) }}
    {{ form_row(form.fullName) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password.first) }}
    {{ form_row(form.password.second) }}
    {{ form_end(form) }}

{% endblock %}
